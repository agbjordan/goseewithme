{
  "name": "goseewithme",
  "version": "1.0.0",
  "description": "Guide Tour E-commerce and Social Media Site",
  "main": "server.js",
  "scripts": {
    "build": "cd ./client && react-scripts build",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "concurrently \"npm run start\" \"npm run serve\"",
    "start": "node server.js",
    "server": "nodemon server.js",
    "serve": "NODE_ENV=production node ./client/server/index.js"
  },
  "author": "Andrew Jordan",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "flat": "^4.1.0",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^5.2.13",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.6.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.3"
  }
}
